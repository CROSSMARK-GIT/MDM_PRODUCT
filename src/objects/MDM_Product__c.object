    <validationRules>
        <fullName>ConsumerProduct_EnterCaseUPC_CassPack</fullName>
        <active>true</active>
        <description>If Consumer Product is selected and any of the remaining, the user should probably provide a case UPC and a</description>
        <errorConditionFormula>OR
(AND(Is_Consumer_Product__c == true,
ISNULL(Case_Pack__c),IgnoreWarnings__c == false),
AND(Is_Consumer_Product__c == true, ISBLANK(Case_UPC__c),   IgnoreWarnings__c== false ))</errorConditionFormula>
        <errorMessage>Warning: Consumer Product is selected Provide a case UPC and a CasePack.
To Ignore the warning and proceed, Accept Ignore Warnings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HeightWidthDepth_DimensionUnitofMeasure</fullName>
        <active>true</active>
        <errorConditionFormula>And(
OR( Package_Depth__c &lt;&gt; 0, Package_Height__c &lt;&gt; 0, Package_Width__c &lt;&gt; 0),  
( ISPICKVAL(Dimension_Unit_of_Measure__c,&quot;&quot;)))</errorConditionFormula>
        <errorMessage>If a height, width, or depth dimension are entered, there must be a dimension unit of measure selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdentifiersValidation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK(EAN8__c)),
    LEN(EAN8__c) &lt; 8),
AND(NOT(ISBLANK(UPC12__c)),
    LEN(UPC12__c) &lt; 12),
And (NOT(ISBLANK(EAN13__c)),
    LEN(EAN13__c) &lt; 13),
AND (NOT(ISBLANK(EAN14__c)),
    LEN(EAN14__c) &lt; 14)
)</errorConditionFormula>
        <errorMessage>Invalid Identifier in EAN8/UPC12/EAN13/EAN14</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OneoftheDomainUsagecheckboxesChecked</fullName>
        <active>true</active>
        <description>At least one of the Domain/Usage check boxes should be checked</description>
        <errorConditionFormula>AND(
AND( Is_Consumer_Product__c == false, Is_Planning_Product__c == false, Is_Orderable_Product__c == false, Is_Shipping_Product__c == false, Is_Invoicable_Product__c == false),
(IgnoreWarnings__c = false))</errorConditionFormula>
        <errorMessage>Warning: At least one of the Domain/Usage checkboxes should be checked.
To Ignore the warning and proceed, Accept Ignore Warnings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanningProductSelected_EnterCostofGood</fullName>
        <active>true</active>
        <description>At least one of the Domain/Usage check boxes should be checked</description>
        <errorConditionFormula>OR(
AND(Is_Planning_Product__c == true,
Cost_of_Goods_Sold__c == 0.0,IgnoreWarnings__c == false),
AND(Is_Planning_Product__c == true,
ISNULL(Cost_of_Goods_Sold__c),IgnoreWarnings__c == false)
)</errorConditionFormula>
        <errorMessage>Warning: Planning Product check box is selected, Enter a Cost of Good sold value.
To Ignore the warning and proceed, Accept Ignore Warnings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule1</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(OR(REGEX(EAN13__c , &quot;^[0-9]{10}&quot;)))</errorConditionFormula>
        <errorDisplayField>EAN13__c</errorDisplayField>
        <errorMessage>Invalid UPC</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Weight_WeightUnitofMeasure</fullName>
        <active>true</active>
        <errorConditionFormula>And(
OR( Package_Weight__c &lt;&gt; 0 ),  
( ISPICKVAL(Weight_Unit_of_Measure__c ,&quot;&quot;)))</errorConditionFormula>
        <errorMessage>If a weight value is entered, a weight unit of measure must be selected</errorMessage>
    </validationRules>
